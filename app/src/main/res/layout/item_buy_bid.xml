<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/_5sdp"
    android:background="@drawable/new_style_container">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <ImageView
            android:id="@+id/SellerListAdapterImgUnFav"
            android:layout_width="@dimen/_20sdp"
            android:layout_height="@dimen/_20sdp"
            android:layout_marginLeft="@dimen/_220sdp"
            android:src="@drawable/yellow"
            android:visibility="gone" />


        <LinearLayout

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="visible">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Posted At:"
                android:textColor="@color/color_label"
                android:visibility="visible" />

            <TextView
                android:id="@+id/posted_at"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:text=""
                android:textColor="@color/color_black"
                android:visibility="visible" />


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp"
            android:orientation="horizontal">


            <LinearLayout
                android:id="@+id/typeIdLv"
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:orientation="horizontal"
                android:visibility="gone">

                <TextView

                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight=".8"
                    android:text="Type:"
                    android:textColor="@color/color_label"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/SellBidFragmentTxtType"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.8"
                    android:text="Type"
                    android:textColor="@color/color_black"
                    android:visibility="gone" />


            </LinearLayout>


            <LinearLayout
                android:id="@+id/idLv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:visibility="visible">


                <TextView

                    android:id="@+id/id_tv"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight=".2"
                    android:text="Seller Id:"
                    android:textColor="@color/color_label"
                    android:visibility="visible" />

                <TextView
                    android:id="@+id/id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:text="Booking Id"
                    android:textColor="@color/color_black"
                    android:visibility="visible" />


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/crc55"
                    android:text="Mill :"
                    android:textColor="@color/color_label"
                    android:textSize="@dimen/_12sdp" />

                <TextView
                    android:id="@+id/SellBidFragmentTxtMillName"
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:text="Prosola Technologyu"
                    android:textColor="@color/color_black" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_20sdp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/crc55"
                    android:text="Time Left :"
                    android:textColor="@color/color_label"
                    android:textSize="@dimen/_12sdp" />

                <TextView
                    android:id="@+id/SellBidFragmentTxtTime"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight=".8"
                    android:fontFamily="@font/crc55"
                    android:text="12:00"
                    android:textColor="@color/color_black"
                    android:textSize="@dimen/_12sdp" />

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/_8sdp"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight=".6"
                        android:fontFamily="@font/crc55"
                        android:text="Grade :"
                        android:textColor="@color/color_label"
                        android:textSize="@dimen/_12sdp" />

                    <TextView
                        android:id="@+id/SellBidFragmentTxtGrade"
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight=".8"
                        android:fontFamily="@font/crc55"
                        android:text=""
                        android:textColor="@color/color_black"
                        android:textSize="@dimen/_12sdp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/SellBidFragmentLinearLayoutRate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight=".9"
                        android:fontFamily="@font/crc55"
                        android:text="Rate/Qtl : "
                        android:textColor="@color/color_label"
                        android:textSize="@dimen/_12sdp" />

                    <TextView
                        android:id="@+id/SellBidFragmentTxtRate"
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight=".9"
                        android:fontFamily="@font/crc55"
                        android:text=""
                        android:textColor="@color/color_black"
                        android:textSize="@dimen/_12sdp" />


                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/SellBidFragmentLinearLayoutClaimed"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_23sdp"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight=".5"
                        android:fontFamily="@font/crc55"
                        android:text="Claimed :"
                        android:textColor="@color/color_blue"
                        android:textSize="@dimen/_12sdp" />

                    <TextView
                        android:id="@+id/SellBidFragmentTxtClaimed"
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight=".8"
                        android:fontFamily="@font/crc55"
                        android:text=""
                        android:textColor="@color/color_black"
                        android:textSize="@dimen/_12sdp" />
                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_23sdp"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/SellBidFragmentTxtQty"
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight=".9"
                        android:fontFamily="@font/crc55"
                        android:text="Available Qty : "
                        android:textColor="@color/color_label"
                        android:textSize="@dimen/_12sdp" />

                    <TextView
                        android:id="@+id/SellBidFragmentTxtAvailableQty"
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight=".6"
                        android:fontFamily="@font/crc55"
                        android:text=""
                        android:textColor="@color/color_black"
                        android:textSize="@dimen/_12sdp" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>



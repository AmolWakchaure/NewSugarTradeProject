<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/_5sdp"
    android:orientation="horizontal">


    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="9"
        android:background="@drawable/new_style_container"
        android:orientation="vertical">


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/id_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/crc55"
                    android:text=" "
                    android:textColor="@color/color_label"
                    android:textSize="@dimen/_12sdp" />

                <TextView
                    android:id="@+id/id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/crc55"
                    android:text="Id"
                    android:textColor="@color/color_black"
                    android:textSize="@dimen/_12sdp" />

            </LinearLayout>


        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/crc55"
                android:text="Posted at : "
                android:textColor="@color/color_label"
                android:textSize="@dimen/_12sdp" />

            <TextView
                android:id="@+id/MyOfferActivityTxtDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/crc55"
                android:text="gjgjhghjg"
                android:textColor="@color/color_black"
                android:textSize="@dimen/_12sdp" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/typeTxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                 android:visibility="gone"
                android:text="Type : "
                android:textSize="@dimen/_12sdp"
                android:fontFamily="@font/crc55"
                android:textColor="@color/color_label"
                 />


            <TextView
                android:id="@+id/MyOfferAdapterTxtType"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:text="Type"
                android:fontFamily="@font/crc55"
                android:textColor="@color/color_black"
                android:textStyle="bold"
                 />


        </LinearLayout>








        <LinearLayout
            android:id="@+id/startTime_li"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/crc55"
                android:text="Start Time : "
                android:textColor="@color/color_label"
                android:textSize="@dimen/_12sdp" />

            <TextView
                android:id="@+id/start_time_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_3sdp"
                android:fontFamily="@font/crc55"
                android:text=""
                android:textColor="@color/color_black"
                android:textSize="@dimen/_12sdp" />

        </LinearLayout>




        <LinearLayout
            android:id="@+id/endTime_li"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/crc55"
                android:text="End Time : "
                android:textColor="@color/color_label"
                android:textSize="@dimen/_12sdp" />

            <TextView
                android:id="@+id/end_time_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_3sdp"
                android:fontFamily="@font/crc55"
                android:text=""
                android:textColor="@color/color_black"
                android:textSize="@dimen/_12sdp" />

        </LinearLayout>




        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/orginal_required_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/crc55"
                android:visibility="gone"
                android:text="Original Required Qty : "
                android:textColor="@color/color_label"
                android:textSize="@dimen/_12sdp" />

            <TextView
                android:id="@+id/original_required_et"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_3sdp"
                android:fontFamily="@font/crc55"
                android:text=""
                android:visibility="gone"
                android:textColor="@color/color_black"
                android:textSize="@dimen/_12sdp" />

        </LinearLayout>



        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/org"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/crc55"
                android:text="Original Sell Qty : "
                android:textColor="@color/color_label"
                android:textSize="@dimen/_12sdp" />

            <TextView
                android:id="@+id/org_sell_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_3sdp"
                android:fontFamily="@font/crc55"
                android:text=""
                android:textColor="@color/color_black"
                android:textSize="@dimen/_12sdp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/qty_val"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/crc55"
                android:text="Required Qty : "
                android:textColor="@color/color_label"
                android:textSize="@dimen/_12sdp" />

            <TextView
                android:id="@+id/MyOfferActivityTxtQuantity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_3sdp"
                android:fontFamily="@font/crc55"
                android:text="hjvgjhvhjv"
                android:textColor="@color/color_black"
                android:textSize="@dimen/_12sdp" />

        </LinearLayout>




        <LinearLayout
            android:id="@+id/aquired_qty_li"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/aqquired_required_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/crc55"
                android:text="Acquired Qty : "
                android:textColor="@color/color_label"
                android:textSize="@dimen/_12sdp" />

            <TextView
                android:id="@+id/aqquired_required_et"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_3sdp"
                android:fontFamily="@font/crc55"
                android:text="bcc"
                android:textColor="@color/color_black"
                android:textSize="@dimen/_12sdp" />

        </LinearLayout>





        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/cliamed"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/crc55"
                android:text="Claimed Qty : "
                android:textColor="@color/color_label"
                android:textSize="@dimen/_12sdp" />

            <TextView
                android:id="@+id/claimed_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_3sdp"
                android:fontFamily="@font/crc55"
                android:text=""
                android:textColor="@color/color_black"
                android:textSize="@dimen/_12sdp" />

        </LinearLayout>



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:fontFamily="@font/crc55"
                    android:text="Grade : "
                    android:textColor="@color/color_label"
                    android:textSize="@dimen/_12sdp" />

                <TextView
                    android:id="@+id/MyOfferActivityTxtGrade"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/crc55"
                    android:text="gfhfghfgh"
                    android:textColor="@color/color_black"
                    android:textSize="@dimen/_12sdp" />

            </LinearLayout>


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">


            <LinearLayout
                android:id="@+id/MyOfferAdapterLInearLayoutBlank"
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_weight=".9"
                android:orientation="horizontal"
                android:visibility="gone">

                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight=".4"
                    android:fontFamily="@font/crc55"
                    android:textColor="@color/color_label"
                    android:textSize="@dimen/_10sdp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:fontFamily="@font/crc55"
                    android:textColor="@color/color_black"
                    android:textSize="@dimen/_12sdp" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/MyOfferAdapterLInearLayoutRate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/crc55"
                    android:text="Price /Qtl : "
                    android:textColor="@color/color_label"
                    android:textSize="@dimen/_12sdp"/>

                <TextView
                    android:id="@+id/MyOfferActivityTxtRate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/crc55"
                    android:text="jhvgjh"
                    android:textColor="@color/color_black"
                    android:textSize="@dimen/_12sdp" />

            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/MyOfferActivityLinearLayoutRevert"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_marginRight="@dimen/_2sdp"
        android:layout_weight="1"
        android:background="@drawable/new_style_button"
        android:gravity="center"
        android:orientation="vertical"
        android:visibility="visible">

        <ImageView
            android:layout_width="@dimen/_20sdp"
            android:layout_height="@dimen/_20sdp"
            android:layout_gravity="center"
            android:background="@drawable/forward"
            android:backgroundTint="@color/color_white" />
    </LinearLayout>
</LinearLayout>
